     void   write(int X,int  Y)
	{
	P->x=X;
	P->y=Y;
	P=P->Next;

	}
      
     void   Retrieve( Position L )
        { 
	Position P=L->Next;
	  while(P!=L)
	{
	printf(" x%d.y%d ",P->x,P->y);
	P=P->Next;
	}
	  printf("\n");
        }

	 void LMakeEmpty(List L)
	{
		Position temp, p=L->Next;
	  while(p!=L)
	{
	temp=p;
	free(temp);
	p=p->Next;
	}
	L->Next=L;

	}












